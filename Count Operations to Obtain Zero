// Similar to Euclidean Algorthm - Recursive

class Solution {
public:
    int countOperations(int num1, int num2) {
        if(num1 == 0 || num2 == 0)
            return 0;

        if(num1 < num2)
            swap(num1, num2);

        return num1 / num2 + countOperations(num1 % num2, num2);
    }
};

// TC : O(log(min(num1, num2)))
// SC : O(1)
