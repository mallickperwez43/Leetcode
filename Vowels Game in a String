// STL + lamda(explicit)

class Solution {
public:
    bool doesAliceWin(string s) {
        auto lambda = [&](char ch) {
            return string("aeiou").find(ch) != string :: npos;
        };

        return any_of(s.begin(), s.end(), lambda);
    }
};

// STL + lamnda(implicit)

class Solution {
public:
    bool doesAliceWin(string s) {
        return any_of(s.begin(), s.end(), [&](char ch) {
            return string("aeiou").find(ch) != string :: npos;
        });
    }
};
